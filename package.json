{
  "name": "@matthew-gh-org/ts-app-template",
  "version": "0.1.0",
  "description": "TypeScript Application Template",
  "main": "index.js",
  "repository": "https://github.com/matthew-gh-org/ts-app-template.git",
  "author": "Matthew Ondash <ondashm@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "tsc --build tsconfig.json",
    "check:lint": "yarn lint .",
    "check:style": "yarn style --check .",
    "check:types": "tsc --build tsconfig.json",
    "clean:build": "rimraf build",
    "dev": "ts-node -r dotenv/config --files src",
    "fix:style": "yarn style --write .",
    "start": "nodemon --config config/nodemon.json",
    "test": "jest --config config/jest.config.ts",
    "test:ci": "yarn test --ci",
    "test:watch": "yarn test --watch",
    "lint": "eslint --config config/.eslintrc.yml --ignore-path config/.eslintignore",
    "style": "prettier --config config/.prettierrc.json --ignore-path config/.prettierignore"
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/node": "^14.17.1",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "codecov": "^3.8.2",
    "dotenv": "^10.0.0",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.23.3",
    "eslint-plugin-jest": "^24.3.6",
    "jest": "^27.0.1",
    "jest-watch-typeahead": "^0.6.3",
    "nodemon": "^2.0.7",
    "prettier": "^2.3.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.0",
    "ts-node": "^10.0.0",
    "typescript": "^4.2.4"
  }
}
